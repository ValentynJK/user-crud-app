{
  "name": "pet-crud-app",
  "version": "1.0.0",
  "description": "A simple CRUD application for managing pets using Express.js, Prisma ORM, and PostgreSQL.",
  "main": "src/app.ts",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "db:create": "ts-node src/prisma/scripts/createDatabase.ts",
    "studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^3.0.0",
    "dotenv": "^17.2.1",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "prisma": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT"
}
